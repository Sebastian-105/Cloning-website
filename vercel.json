{
    "version": 2,
    "builds": [
      {
        "src": "proxy.js",
        "use": "@now/node"
      }
    ],
    "routes": [
        {
            "src": "/home",
            "status": 308,
            "headers": { "Location": "https://sebastian-105.vercel.app" }
          },
      {
        "src": "/(.*)",
        "dest": "proxy.js"
      }
    ]
  }